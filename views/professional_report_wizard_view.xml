<odoo>
    <!--  Vista del wizard  -->
    <record id="view_professional_report_wizard" model="ir.ui.view">
        <field name="name">professional.report.wizard.form</field>
        <field name="model">professional.report.wizard</field>
        <field name="arch" type="xml">
            <form string="Opciones de Reporte">
                <group>
                    <field name="include_personal_info" />
                    <field name="include_specialities" />
                    <field name="include_experience" />
                    <field name="include_availability" />
                    <field name="include_statistics" />
                    <field name="include_participants" />
                </group>
                <footer>
                    <button string="Generar" name="action_print_report" type="object"
                        class="btn-primary" />
                    <button string="Cancelar" class="btn-secondary" special="cancel" />
                </footer>
            </form>
        </field>
    </record>
</odoo>